    <script type="text/javascript">
        $(document).ready(function () {
            $('#myDataTable').DataTable({
                "processing": true,
                "serverSide": true,
                "ajax": {
                    "url": "@string.Format("{0}://{1}{2}", Request.Url.Scheme, Request.Url.Authority, Url.Content("~"))/Base/AjaxHandlerToGetLogs",
            "type": "POST"
        },
            "columns": [
                { "data": "Id" },
                { "data": "Date" },
                { "data": "Thread" },
                { "data": "Level" },
                { "data": "Logger" },
                { "data": "Message" },
                { "data": "Exception" }
            ]
        });
        });

